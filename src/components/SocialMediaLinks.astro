---
import {socialMediaLinks} from "@data/userData"
---

<div class="flex items-center justify-evenly w-full max-w-72 mt-5">
    {socialMediaLinks.map((link) => (
        <a
            href={link.url}
            target="_blank"
            rel="noopener noreferrer"
            class="text-gray-500 hover:text-gray-700"
        >
           <img src={link.icon} alt={link.name} class="w-6 h-6" />
        </a>
    ))}
</div>